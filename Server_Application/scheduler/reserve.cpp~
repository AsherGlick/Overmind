#include <string.h>
#include "reserve.h"
using namespace std;

reserve::reserve(int nreserveTime, int nmonth, int nday, int nyear, int nid, string nname)
{
	reserveTime = nreserveTime;
	month = nmonth;
	day = nday;
	year = nyear;
	id = nid;
	name = nname;
}
int reserve::getMonth()
{
	return month;
}
int reserve::getDay()
{
	return day;
}
int reserve::getYear()
{
	return year;
}
int reserve::getId()
{
	return id;
}
int reserve::getReserveTime()
{
	return reserveTime;
}
string reserve::getName()
{
	return name;
}
